<?php
if (!empty($this->error)) {
	echo $this->error;
}
else if (!empty($this->data) && is_array($this->data)) {
	foreach ($this->data as $row) {
		//use of the default render, but any other display process could have
		//been implemented, for example, because it is a HTML view, the rows
		//could be displayed in a HTML table
		XMLParser_File_Sales::renderRow($row);
	}
}
